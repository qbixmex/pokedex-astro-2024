{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@root/*": ["./*"],
      "@src/*": ["./src/*"],
      "@icons/*": ["./src/icons/*"],
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@services/*": ["./src/services/*"],
      "@data/*": ["./src/data/*"],
      "@interfaces/*": ["./src/interfaces/*"],
      "@enums/*": ["./src/enums/*"],
      "@utils/*": ["./src/utils/*"],
    }
  }
}